{
  "expo": {
    "name": "TimesEra_Retail",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/TTTTT.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "81bbc233-6d7b-4438-9ac8-dd6a9b47aecc"
      }
    },
    "android": {
      "package": "com.raviteja960.boltexponativewind",
      "permissions": ["INTERNET"],
      "usesCleartextTraffic": true
    }
  }
}
